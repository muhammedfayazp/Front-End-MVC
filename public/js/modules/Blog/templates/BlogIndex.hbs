<div class="ui grid">
	<div class="column eight wide">
		<h3 class="ui header">Blogs</h3>
	</div>
	<div class="column eight wide">
		<a class="blog-button blog-add-button ui button">Add</a>
	</div>
</div>
<table class="ui celled table segment">
	<tbody>
		{{#each blogs}}
			<tr data-id="{{id}}" class="blog-show">
				<td>{{title}}</td>
			</tr>
		{{/each}}
	</tbody>
</table>